{
  "name": "my-cv",
  "private": true,
  "version": "0.0.23",
  "type": "module",
  "scripts": {
    "setup": "yarn --silent --ignore-scripts --emoji --immutable",
    "dev": "next dev --turbopack",
    "prebuild": "rimraf dist .next build",
    "build": "yarn prebuild && tsc && next build",
    "format": "prettier . -w",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 && stylelint **/*.css && yarn type-check",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "ci": "yarn build && yarn lint && yarn test",
    "clear": "rimraf node_modules build .next"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-countup": "6.5.3",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.20.0",
    "@next/eslint-plugin-next": "15.1.6",
    "@tailwindcss/postcss": "4.0.5",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "@typescript-eslint/parser": "8.23.0",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "9.19.0",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "gh-pages": "6.3.0",
    "globals": "15.14.0",
    "jsdom": "26.0.0",
    "next": "15.1.6",
    "postcss": "8.5.1",
    "prettier": "3.5.0",
    "rimraf": "^6.0.1",
    "stylelint": "16.14.1",
    "stylelint-config-standard": "37.0.0",
    "tailwindcss": "4.0.4",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5"
  },
  "homepage": "https://fadihanna123.github.io/my-cv"
}
