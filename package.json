{
  "name": "my-cv",
  "private": true,
  "version": "0.0.12",
  "type": "module",
  "scripts": {
    "setup": "yarn --silent --ignore-scripts --emoji --immutable",
    "dev": "vite",
    "prebuild": "rimraf dist",
    "format": "prettier . -w",
    "build": "yarn prebuild && tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 && stylelint **/*.css",
    "preview": "vite preview",
    "deploy": "yarn setup && yarn build && gh-pages -d dist",
    "test": "vitest",
    "ci": "yarn build && yarn lint",
    "clear": "rimraf node_modules dist"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-chrono": "^2.6.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.2.0",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.15",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "vite-plugin-minify": "^2.1.0",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "homepage": "https://fadihanna123.github.io/my-cv"
}
