{
  "name": "my-cv",
  "private": true,
  "version": "0.0.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "format": "prettier . -w",
    "setup": "yarn --silent --emoji --ignore-scripts --imutable",
    "clear": "rimraf node_modules dist",
    "build": "yarn prebuild && tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 && stylelint **/*.css",
    "preview": "vite preview",
    "deploy": "yarn && yarn build && gh-pages -d dist",
    "test": "vitest",
    "ci": "yarn build && yarn lint"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "react": "^18.3.1",
    "react-chrono": "^2.6.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.14",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10",
    "vite-plugin-minify": "^2.0.1",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.1.4"
  },
